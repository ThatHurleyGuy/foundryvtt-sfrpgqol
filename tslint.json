{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "semicolon": false,
    "max-line-length": [true, 200],
    "interface-name": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration"
    ],
    "variable-name": {
      "options": [
        "ban-keywords"
      ]
    }
  },
  "rulesDirectory": []
}
